{
  "name": "CS2BrowserLauncher",
  "productName": "CS2Browser - Launcher",
  "version": "0.2.6",
  "description": "CS2Browser - Launcher - Download manager and Launcher for CS2",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "electron-builder build --win --x64"
  },
  "keywords": [],
  "author": {
    "name": "CS2Browser.com",
    "email": "info@cs2browser.com"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@electron-forge/plugin-webpack": "^6.4.2",
    "@electron-forge/publisher-github": "^6.4.2",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.0.0",
    "electron": "26.2.4",
    "electron-builder": "^24.6.4",
    "electron-builder-squirrel-windows": "^24.7.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@popperjs/core": "^2.11.8",
    "autoprefixer": "^10.4.16",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "electron-dl": "^3.5.1",
    "electron-forge-publisher-generic": "^0.0.7",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.4",
    "file-loader": "^6.2.0",
    "jquery": "^3.7.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.31",
    "postcss-loader": "^4.2.0",
    "sass": "^1.69.0",
    "sass-loader": "^10.1.1",
    "steam-game-path": "^2.2.0",
    "steam-server-query": "^1.1.3",
    "update-electron-app": "^2.0.1"
  },
  "browser": {
    "child_process": false
  },
  "build": {
    "appId": "com.cs2browser.launcher",
    "productName": "CS2Browser - Launcher",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "perMachine": true,
      "allowElevation": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CS2Browser/Launcher.git"
  }
}
